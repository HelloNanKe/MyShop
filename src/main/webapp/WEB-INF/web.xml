<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>RegisterUIServlet</servlet-name>
        <servlet-class>com.zt.web.UI.RegisterUIServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.zt.web.controller.registerServlet</servlet-class>
    </servlet>

   
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.zt.web.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginUIServlet</servlet-name>
        <servlet-class>com.zt.web.UI.LoginUIServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutUIServlet</servlet-name>
        <servlet-class>com.zt.web.UI.LogoutUIServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutUIServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
        
    <servlet-mapping>
        <servlet-name>RegisterUIServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginUIServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>
